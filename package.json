{
  "name": "@jgithub/ts-gist-pile",
  "version": "0.0.319",
  "description": "tired of finding this on stackoverflow",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./opentelemetry/api": {
      "types": "./dist/opentelemetry/api/index.d.ts",
      "default": "./dist/opentelemetry/api/index.js"
    }
  },
  "devDependencies": {
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.5",
    "@types/uuid": "^10.0.0",
    "chai": "^4.3.7",
    "csv": "^6.4.1",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "prepack": "npm run build",
    "prepublishOnly": "npm test",
    "build": "tsc",
    "test": "env TZ='America/Chicago' env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'test/**/*.test.ts'",
    "publish:github": "npm publish --registry https://npm.pkg.github.com",
    "publish:npm": "npm publish --registry https://registry.npmjs.org --access public",
    "publish:both": "npm run publish:github && npm run publish:npm"
  },
  "files": [
    "dist/**/*",
    "opentelemetry"
  ],
  "keywords": [],
  "license": "MIT",
  "author": "jgithub",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jgithub/ts-gist-pile.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "uuid": "^11.1.0"
  }
}
